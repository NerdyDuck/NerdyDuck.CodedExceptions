<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata minClientVersion="3.2">
		<id>NerdyDuck.CodedExceptions</id>
		<version>1.1.1</version>
		<title>NerdyDuck CodedExceptions</title>
		<authors>Daniel Kopp</authors>
		<description>This library aims to provide a set of the most common exceptions, with constructors allowing to set the HResult property value. In addition, it provides tools to create standardized error codes, and to discover if an exception is able to provide custom error codes.</description>
		<releaseNotes>
			v1.1.1: Fixed a bug in NuGet package that prevented adding the PRI file to a UWP project deployment.
		</releaseNotes>
		<summary>Exceptions with custom HRESULTs for .NET 4.6+ and UWP.</summary>
		<language>en-US</language>
		<projectUrl>https://github.com/NerdyDuck/NerdyDuck.CodedExceptions</projectUrl>
		<iconUrl>http://static.nerdyduck.de/NuGet/NerdyDuck.CodedExceptions-50x50.png</iconUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<licenseUrl>http://opensource.org/licenses/Apache-2.0</licenseUrl>
		<copyright>Copyright Daniel Kopp 2015</copyright>
		<frameworkAssemblies>
			<frameworkAssembly assemblyName="System.Configuration" targetFramework="net46" />
			<frameworkAssembly assemblyName="System.Data" targetFramework="net46" />
			<frameworkAssembly assemblyName="System.ServiceModel" targetFramework="net46" />
			<frameworkAssembly assemblyName="System.Xml" targetFramework="net46" />
		</frameworkAssemblies>

		<dependencies></dependencies>
		<references></references>
		<tags>exception error hresult uwp desktop</tags>
		<!-- Looks like content files are still not supported in nuget 3.3 -->
		<contentFiles>
			<files include="any\any\ErrorCodes.csv" buildAction="None" copyToOutput="false" />
			<files include="cs\any\Errors.cs.pp" buildAction="Compile" />
			<files include="vb\any\Errors.vb" buildAction="Compile" />
		</contentFiles>
	</metadata>
	<files>
		<!-- Dummy file to mark package usable for UAP also in VS package manager (actual targeting for UAP must be done using the ref and runtimes folders) -->
		<file src="..\..\src\CodedExceptionsUniversal\bin\Release\NerdyDuck.CodedExceptions.xml" target="lib\uap10.0\" />

		<!-- Simple Txm for .Net 4.5 desktop version-->
		<file src="..\..\src\CodedExceptionsDesktop\bin\Release\NerdyDuck.CodedExceptions.dll" target="lib\net46\" />
		<file src="..\..\src\CodedExceptionsDesktop\bin\Release\NerdyDuck.CodedExceptions.pdb" target="lib\net46\" />
		<file src="..\..\src\CodedExceptionsDesktop\bin\Release\NerdyDuck.CodedExceptions.xml" target="lib\net46\" />
		<file src="..\..\src\CodedExceptionsDesktop\bin\Release\de-DE\NerdyDuck.CodedExceptions.resources.dll" target="lib\net46\de-DE\" />

		<!-- Reference files for UAP design-time -->
		<file src="..\..\src\CodedExceptionsUniversal\bin\Release\NerdyDuck.CodedExceptions.dll" target="ref\uap10.0\" />
		<file src="..\..\src\CodedExceptionsUniversal\bin\Release\NerdyDuck.CodedExceptions.xml" target="ref\uap10.0\" />
		<file src="..\..\src\CodedExceptionsUniversal\bin\Release\NerdyDuck.CodedExceptions.pri" target="ref\uap10.0\" />
		<file src="..\..\src\CodedExceptionsUniversal\CodedExceptions.rd.xml" target="ref\uap10.0\NerdyDuck.CodedExceptions\" />

		<!-- Platform-specific files for UAP compile-time and run-time -->
		<file src="..\..\src\CodedExceptionsUniversal\CodedExceptions.rd.xml" target="runtimes\win10-arm\lib\uap10.0\NerdyDuck.CodedExceptions\" />
		<file src="..\..\src\CodedExceptionsUniversal\bin\ARM\Release\NerdyDuck.CodedExceptions.dll" target="runtimes\win10-arm\lib\uap10.0\" />
		<file src="..\..\src\CodedExceptionsUniversal\bin\ARM\Release\NerdyDuck.CodedExceptions.pdb" target="runtimes\win10-arm\lib\uap10.0\" />
		<file src="..\..\src\CodedExceptionsUniversal\bin\ARM\Release\NerdyDuck.CodedExceptions.pri" target="runtimes\win10-arm\lib\uap10.0\" />
		<file src="..\..\src\CodedExceptionsUniversal\CodedExceptions.rd.xml" target="runtimes\win10-x86\lib\uap10.0\NerdyDuck.CodedExceptions\" />
		<file src="..\..\src\CodedExceptionsUniversal\bin\x86\Release\NerdyDuck.CodedExceptions.dll" target="runtimes\win10-x86\lib\uap10.0\" />
		<file src="..\..\src\CodedExceptionsUniversal\bin\x86\Release\NerdyDuck.CodedExceptions.pdb" target="runtimes\win10-x86\lib\uap10.0\" />
		<file src="..\..\src\CodedExceptionsUniversal\bin\x86\Release\NerdyDuck.CodedExceptions.pri" target="runtimes\win10-x86\lib\uap10.0\" />
		<file src="..\..\src\CodedExceptionsUniversal\CodedExceptions.rd.xml" target="runtimes\win10-x64\lib\uap10.0\NerdyDuck.CodedExceptions\" />
		<file src="..\..\src\CodedExceptionsUniversal\bin\x64\Release\NerdyDuck.CodedExceptions.dll" target="runtimes\win10-x64\lib\uap10.0\" />
		<file src="..\..\src\CodedExceptionsUniversal\bin\x64\Release\NerdyDuck.CodedExceptions.pdb" target="runtimes\win10-x64\lib\uap10.0\" />
		<file src="..\..\src\CodedExceptionsUniversal\bin\x64\Release\NerdyDuck.CodedExceptions.pri" target="runtimes\win10-x64\lib\uap10.0\" />

		<!-- Source files for the symbols package -->
		<file src="..\..\src\CodedExceptionsDesktop\**\*.cs" exclude="..\..\src\CodedExceptionsDesktop\obj\**\*.cs" target="src\CodedExceptionsDesktop\" />
		<file src="..\..\src\CodedExceptionsUniversal\**\*.cs" exclude="..\..\src\CodedExceptionsUniversal\obj\**\*.cs" target="src\CodedExceptionsUniversal\" />
	</files>
</package>
